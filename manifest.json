{
    "manifest_version": 2,
    "name": "QR Code Generator",
    "version": "0.0.1",
    "icons": {
        "16": "images/QRScanner16.png",
        "48":"images/QRScanner48.png",
        "128":"images/QRScanner128.png"
    },
    "description": "Creates the QR code of the current URL you are in.",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions":[
        "tabs"
    ]
}